/* Q114 (Logic Enhancers)
Length of longest substring without repeating characters.
*/
#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    int n, i, j, maxLen=0;
    scanf("%s", s);
    n = strlen(s);
    for(i=0; i<n; i++) {
        int visited[256] = {0};
        for(j=i; j<n; j++) {
            if(visited[(int)s[j]] == 1) break;
            visited[(int)s[j]] = 1;
            if(j-i+1 > maxLen) maxLen = j-i+1;
        }
    }
    printf("%d", maxLen);
    return 0;
}
