/* Q112 (Logic Enhancers)
Kadane's algorithm for maximum contiguous subarray sum.
*/
#include <stdio.h>
int main() {
    int n, i, max_so_far=-99999, current_max=0;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++) {
        scanf("%d", &arr[i]);
    }
    for(i=0; i<n; i++) {
        current_max += arr[i];
        if(max_so_far < current_max) max_so_far = current_max;
        if(current_max < 0) current_max = 0;
    }
    printf("%d", max_so_far);
    return 0;
}
