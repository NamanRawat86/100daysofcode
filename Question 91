/* Q91 (Strings)
Remove all vowels from a string.
*/
#include <stdio.h>
#include <ctype.h>
int main() {
    char s[100];
    int i=0;
    scanf("%s", s);
    while(s[i] != '\0') {
        char ch = tolower(s[i]);
        if(ch!='a' && ch!='e' && ch!='i' && ch!='o' && ch!='u') {
            printf("%c", s[i]);
        }
        i++;
    }
    return 0;
}
