/* Q76 (2D Arrays)
Check if a matrix is symmetric.
*/
#include <stdio.h>
int main() {
    int r, c, i, j, flag=1;
    scanf("%d %d", &r, &c);
    int mat[r][c];
    for(i=0; i<r; i++) {
        for(j=0; j<c; j++) {
            scanf("%d", &mat[i][j]);
        }
    }
    if(r != c) {
        flag = 0;
    } else {
        for(i=0; i<r; i++) {
            for(j=0; j<c; j++) {
                if(mat[i][j] != mat[j][i]) {
                    flag = 0;
                    break;
                }
            }
        }
    }
    printf(flag ? "True" : "False");
    return 0;
}
