/* Q117 (Logic Enhancers)
Merge two sorted arrays.
*/
#include <stdio.h>
int main() {
    int m, n, i, j, temp;
    scanf("%d %d", &m, &n);
    int arr[m+n];
    for(i=0; i<m; i++) {
        scanf("%d", &arr[i]);
    }
    for(i=m; i<m+n; i++) {
        scanf("%d", &arr[i]);
    }
    for(i=0; i<m+n; i++) {
        for(j=i+1; j<m+n; j++) {
            if(arr[i] > arr[j]) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
    for(i=0; i<m+n; i++) {
        printf("%d ", arr[i]);
    }
    return 0;
}
