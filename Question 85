/* Q85 (Strings)
Reverse a string.
*/
#include <stdio.h>
#include <string.h>
int main() {
    char s[100], temp;
    int i, len;
    scanf("%s", s);
    len = strlen(s);
    for(i=0; i<len/2; i++) {
        temp = s[i];
        s[i] = s[len-1-i];
        s[len-1-i] = temp;
    }
    printf("%s", s);
    return 0;
}
