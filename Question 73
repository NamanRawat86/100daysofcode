/* Q73 (2D Arrays)
Find the sum of each row of a matrix and store it in an array.
*/
#include <stdio.h>
int main() {
    int r, c, i, j, sum;
    scanf("%d %d", &r, &c);
    int mat[r][c];
    for(i=0; i<r; i++) {
        sum = 0;
        for(j=0; j<c; j++) {
            scanf("%d", &mat[i][j]);
            sum += mat[i][j];
        }
        printf("Row %d: %d\n", i+1, sum);
    }
    return 0;
}
