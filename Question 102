/* Q102 (Logic Enhancers)
Find the index of the smallest element greater than or equal to x (Ceil).
*/
#include <stdio.h>
int main() {
    int n, i, x, idx=-1;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++) {
        scanf("%d", &arr[i]);
    }
    scanf("%d", &x);
    for(i=0; i<n; i++) {
        if(arr[i] >= x) {
            idx = i;
            break;
        }
    }
    printf("%d", idx);
    return 0;
}
