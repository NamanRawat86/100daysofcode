/* Q101 (Logic Enhancers)
Write a Program to take a sorted array and target. Print first and last occurrence indices.
*/
#include <stdio.h>
int main() {
    int n, i, t, f=-1, l=-1;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++) {
        scanf("%d", &arr[i]);
    }
    scanf("%d", &t);
    for(i=0; i<n; i++) {
        if(arr[i] == t) {
            if(f == -1) f = i;
            l = i;
        }
    }
    printf("%d, %d", f, l);
    return 0;
}
