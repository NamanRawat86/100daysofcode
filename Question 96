/* Q96 (Strings)
Reverse each word in a sentence without changing the word order.
*/
#include <stdio.h>
#include <string.h>
int main() {
    char s[200], word[50];
    int i=0, j=0, k;
    fgets(s, 200, stdin);
    while(s[i] != '\0') {
        if(s[i]!=' ' && s[i]!='\n') {
            word[j++] = s[i];
        } else {
            for(k=j-1; k>=0; k--) printf("%c", word[k]);
            printf(" ");
            j = 0;
        }
        i++;
    }
    for(k=j-1; k>=0; k--) printf("%c", word[k]);
    return 0;
}
