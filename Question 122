/* Q122 (File Handling)
Read file contents using fgets and print to console.
*/
#include <stdio.h>
int main() {
    FILE *fp = fopen("info.txt", "r");
    char str[100];
    if(fp == NULL) return 1;
    while(fgets(str, 100, fp) != NULL) {
        printf("%s", str);
    }
    fclose(fp);
    return 0;
}
