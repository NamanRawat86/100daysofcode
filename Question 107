/* Q107 (Logic Enhancers)
Find the previous greater element for each element.
*/
#include <stdio.h>
int main() {
    int n, i, j, prev;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++) {
        scanf("%d", &arr[i]);
    }
    for(i=0; i<n; i++) {
        prev = -1;
        for(j=i-1; j>=0; j--) {
            if(arr[j] > arr[i]) {
                prev = arr[j];
                break;
            }
        }
        printf("%d, ", prev);
    }
    return 0;
}
