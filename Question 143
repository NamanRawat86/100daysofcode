/* Q143 (Enum)
Find and print the student with the highest marks.
*/
#include <stdio.h>
struct Student { char name[20]; int marks; };
int main() {
    struct Student s[3];
    int i, max=0;
    for(i=0; i<3; i++) {
        scanf("%s %d", s[i].name, &s[i].marks);
        if(s[i].marks > s[max].marks) max = i;
    }
    printf("Topper: %s", s[max].name);
    return 0;
}
