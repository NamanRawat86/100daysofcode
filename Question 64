/* Q64 (Arrays (1D))
Find the digit that occurs the most times in an integer number.
*/
#include <stdio.h>
int main() {
    long long n;
    int freq[10] = {0}, digit, i, max=-1, ans;
    scanf("%lld", &n);
    while(n > 0) {
        digit = n % 10;
        freq[digit]++;
        n /= 10;
    }
    for(i=0; i<10; i++) {
        if(freq[i] > max) {
            max = freq[i];
            ans = i;
        }
    }
    printf("%d", ans);
    return 0;
}
