/* Q92 (Strings)
Find the first repeating lowercase alphabet in a string.
*/
#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    int i, j, len;
    scanf("%s", s);
    len = strlen(s);
    for(i=0; i<len; i++) {
        for(j=i+1; j<len; j++) {
            if(s[i] == s[j]) {
                printf("%c", s[i]);
                return 0;
            }
        }
    }
    return 0;
}
