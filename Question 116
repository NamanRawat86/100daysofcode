/* Q116 (Logic Enhancers)
Two Sum: Find indices where numbers add up to target.
*/
#include <stdio.h>
int main() {
    int n, t, i, j;
    scanf("%d", &n);
    int nums[n];
    for(i=0; i<n; i++) {
        scanf("%d", &nums[i]);
    }
    scanf("%d", &t);
    for(i=0; i<n; i++) {
        for(j=i+1; j<n; j++) {
            if(nums[i] + nums[j] == t) {
                printf("%d %d", i, j);
                return 0;
            }
        }
    }
    printf("-1 -1");
    return 0;
}
