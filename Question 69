/* Q69 (Arrays (1D))
Find the second largest element in an array.
*/
#include <stdio.h>
#include <limits.h>
int main() {
    int n, i, max, sec_max;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++) {
        scanf("%d", &arr[i]);
    }
    max = sec_max = INT_MIN;
    for(i=0; i<n; i++) {
        if(arr[i] > max) {
            sec_max = max;
            max = arr[i];
        } else if(arr[i] > sec_max && arr[i] != max) {
            sec_max = arr[i];
        }
    }
    printf("%d", sec_max);
    return 0;
}
