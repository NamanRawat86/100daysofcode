/* Q142 (Enum)
Store details of 5 students in an array of structures and print all.
*/
#include <stdio.h>
struct Student { char name[20]; int marks; };
int main() {
    struct Student s[5];
    int i;
    for(i=0; i<5; i++) {
        scanf("%s %d", s[i].name, &s[i].marks);
    }
    for(i=0; i<5; i++) {
        printf("%s %d\n", s[i].name, s[i].marks);
    }
    return 0;
}
