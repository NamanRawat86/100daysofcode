/* Q79 (2D Arrays)
Perform diagonal traversal of a matrix.
*/
#include <stdio.h>
int main() {
    int n, i, j;
    scanf("%d %d", &n, &n);
    int mat[n][n];
    for(i=0; i<n; i++) {
        for(j=0; j<n; j++) {
            scanf("%d", &mat[i][j]);
        }
    }
    for(i=0; i<n; i++) {
        printf("%d ", mat[i][i]);
    }
    printf("\n");
    for(i=0; i<n; i++) {
        printf("%d ", mat[i][n-1-i]);
    }
    return 0;
}
