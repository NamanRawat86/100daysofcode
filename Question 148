/* Q146 (Enum)
Create Employee structure with nested Date structure for joining date and print details.
*/
#include <stdio.h>
struct Date { int d, m, y; };
struct Emp { int id; struct Date join; };
int main() {
    struct Emp e = {101, {1, 1, 2023}};
    printf("ID: %d, Date: %d/%d/%d", e.id, e.join.d, e.join.m, e.join.y);
    return 0;
}
