/* Q86 (Strings)
Check if a string is a palindrome.
*/
#include <stdio.h>
#include <string.h>
int main() {
    char s[100];
    int i, len, flag=1;
    scanf("%s", s);
    len = strlen(s);
    for(i=0; i<len/2; i++) {
        if(s[i] != s[len-1-i]) {
            flag = 0;
            break;
        }
    }
    printf(flag ? "Palindrome" : "Not palindrome");
    return 0;
}
