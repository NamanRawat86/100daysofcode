/* Q58 (Arrays (1D))
Find the maximum and minimum element in an array.
*/
#include <stdio.h>
int main() {
    int n, i, min, max;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++) {
        scanf("%d", &arr[i]);
    }
    min = max = arr[0];
    for(i=1; i<n; i++) {
        if(arr[i] > max) max = arr[i];
        if(arr[i] < min) min = arr[i];
    }
    printf("Max=%d, Min=%d", max, min);
    return 0;
}
